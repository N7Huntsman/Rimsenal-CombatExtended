<?xml version="1.0" encoding="utf-8" ?>
<Defs>			
			
			<!-- ==========  Crucible Rifle Projectile =========== -->
			
			<ThingDef Class="CombatExtended.AmmoDef" Name="BaseCrucibleRifleBullet" ParentName="BaseBullet" Abstract="true">
				<graphicData>
					<texPath>Things/Projectile/Laser</texPath>
					<graphicClass>Graphic_Single</graphicClass>
				</graphicData>
				<projectile Class="CombatExtended.ProjectilePropertiesCE">
					<damageDef>Optic</damageDef>
					<speed>80</speed>
					<dropsCasings>false</dropsCasings>
				</projectile>
			</ThingDef>

			<ThingDef Class="CombatExtended.AmmoDef" ParentName="BaseCrucibleRifleBullet">
				<defName>Bullet_CrucibleRifle</defName>
				<label>energy bolt</label>
				<projectile Class="CombatExtended.ProjectilePropertiesCE">
					<damageAmountBase>12</damageAmountBase>
					<armorPenetrationBlunt>3</armorPenetrationBlunt>
					<armorPenetrationSharp>12</armorPenetrationSharp>
				</projectile>
			</ThingDef>
			
