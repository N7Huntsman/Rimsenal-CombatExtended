<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	

	<!-- ================== Projectiles ================== -->

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="BaseProjectileNeolithic" Name="BaseThrowingClubProjectile" Abstract="true">
		<graphicData>
			<texPath>Things/A16/ThrowingClubs</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageDef>Blunt</damageDef>
		</projectile>
	</ThingDef>
	
	<!-- The amount of arrows usable over the ammo lifetime for each resource cost is equal to
	[r / (1 - x)];
		x = preExplosionSpawnChance,
		r = resource cost. -->
	
	<ThingDef Class="CombatExtended.AmmoDef" ParentName="BaseThrowingClubProjectile">
		<defName>Club_Thrown</defName>
		<label>club</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>15</damageAmountBase>
			<speed>12</speed>
			<armorPenetrationBlunt>0.18</armorPenetrationBlunt>
		</projectile>
	</ThingDef>
  
</Defs>
